#include <stdio.h> 
  
// arr is a pointer even if we have 
// use square brackets. 
void printArray(int arr[], int arr_size)  
{ 
    int i; 
    for (i = 0; i < arr_size; i++) { 
        printf("%d ", arr[i]); 
    } 
} 
  
int main() 
{ 
    int arr[4] = { 1, 2, 3, 4 }; 
    int arr_size = sizeof(arr) / sizeof(arr[0]);
    printArray(arr); 
    return 0; 
} 
